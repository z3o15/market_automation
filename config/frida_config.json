{
    "frida": {
        "scriptPath": "/data/local/tmp/market_hook.js",
        "packageName": "com.cyjh.elfin",
        "spawn": true,
        "pause": false,
        "runtime": "v8",
        "enableDebugger": false,
        "debuggerPort": 9222
    },
    "hooks": {
        "network": {
            "enabled": true,
            "interceptOkHttp": true,
            "interceptHttpURLConnection": true,
            "interceptWebView": false,
            "logRequests": true,
            "logResponses": true,
            "filterPatterns": [
                "api/market",
                "equipment",
                "price",
                "item"
            ]
        },
        "ui": {
            "enabled": true,
            "hookClickEvents": true,
            "hookTouchEvents": true,
            "hookListView": true,
            "hookRecyclerView": true,
            "hookActivityLifecycle": true,
            "logViewHierarchy": false,
            "logClickCoordinates": true
        },
        "system": {
            "enabled": false,
            "hookFileOperations": false,
            "hookCryptoOperations": false,
            "hookTimeFunctions": false,
            "hookDebugChecks": true
        }
    },
    "performance": {
        "maxCacheSize": 1000,
        "gcInterval": 300000,
        "enableProfiling": false,
        "logPerformanceMetrics": false,
        "maxMemoryUsage": 512
    },
    "communication": {
        "luaCommFile": "/data/data/com.cyjh.elfin/frida_lua_comm.json",
        "rpcEnabled": true,
        "rpcPort": 27042,
        "commTimeout": 5000,
        "enableWebSocket": false,
        "websocketPort": 8080
    },
    "antiDetection": {
        "enabled": true,
        "hideFridaTraces": true,
        "hideDebugger": true,
        "bypassAntiDebug": true,
        "bypassEmulatorDetection": true,
        "randomizeTiming": true,
        "mimicHumanBehavior": true
    },
    "logging": {
        "level": "INFO",
        "logToFile": false,
        "logPath": "/data/local/tmp/frida_market.log",
        "maxLogSize": "10MB",
        "maxLogFiles": 3,
        "enableVerboseLog": false,
        "logStackTrace": false
    },
    "data": {
        "captureNetworkData": true,
        "captureUIEvents": true,
        "captureUserInteractions": true,
        "autoExport": false,
        "exportFormat": "json",
        "exportPath": "/data/local/tmp/market_data/",
        "compressionEnabled": false
    },
    "security": {
        "encryptCommunication": false,
        "verifyCertificates": true,
        "useCertificatePinning": false,
        "enableSslVerification": true
    },
    "experimental": {
        "enabled": false,
        "features": [
            "advancedUIAnalysis",
            "machineLearningDetection",
            "behavioralAnalysis"
        ]
    }
}